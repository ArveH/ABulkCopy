{
  "AppSettings": {
    "Direction": "in", // In or Out
    "ConnectionString": "Server=localhost;Port=5432;Database=ABulkCopy;User Id=postgres;Integrated Security=true;Include Error Detail=True;",
    "Rdbms": "Pg", // Pg or Mss
    "Folder": "D:\\BulkCopyData",
    "LogFile": "bulkcopy.log",
    "AddQuotes": false,
    "SearchFilter": "", // E.g. "a[sa][ya][sg]%" or "\baimblob_BUG499210\b"
    "EmptyString": "Leave" // Leave, Single, Empty, ForceSingle, ForceEmpty
  },

  "Serilog": {
    "EnableSensitiveDataLogging": false,
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    }
  },

